useMutation(\r\n    (liked) => {\r\n      if (liked) return makeRequest.delete(\"/likes?postId=\" + post.Id);\r\n      return makeRequest.post(\"/likes\", { postId: post.Id });\r\n    },\r\n    {\r\n      onSuccess: () => {\r\n        // Invalidate and refetch\r\n        queryClient.invalidateQueries([\"likes\"]);\r\n      },\r\n    }\r\n  );\r\n\r\n\r\n  const handleLike = () => {\r\n        mutation.mutate(data.includes(currentUser.id));\r\n  };\r\n\r\n\r\n  return (\r\n    <div className=\"post\">\r\n        <div className=\"containerPost\">\r\n        <div className=\"userPost\">\r\n            <div className=\"userInforPost\">\r\n                <img src={\"/upload/\"+post.profilePic} alt=\"\"/>\r\n                <div className=\"details\">\r\n                <Link\r\n                to={`/profile/${post.userId}`}\r\n                style={{ textDecoration: \"none\", color: \"inherit\" }}\r\n              >\r\n                        <span className=\"name\">{post.name}</span>\r\n                    </Link>\r\n                    <span className=\"date\">\r\n                        {moment(post.createdAt).fromNow()}\r\n                    </span>\r\n                </div>\r\n            </div>\r\n            <MoreHorizIcon/>    \r\n        </div>\r\n        <div className=\"content\">\r\n            <p>{post.desc}</p>\r\n            <img src={\"../upload/\"+post.img} alt=\"\"/>\r\n        </div>\r\n        <div className=\"infor\">\r\n            <div className=\"item\">\r\n                <button onClick={handleLike}>\r\n                {isLoading \r\n                ? \"loading\" \r\n                : data.includes(currentUser.id)\r\n                ? <FavoriteOutlinedIcon \r\n                onClick={handleLike}/> \r\n                : <FavoriteBorderOutlinedIcon \r\n                style={{color: \"none\"}}\r\n                onClick={handleLike}\r\n                />}</button>\r\n                {data?.length} likes\r\n            </div>\r\n            <div className=\"item\" onClick={() => setComments(!comments)}>\r\n                <TextsmsOutlinedIcon/>\r\n                Comments\r\n            </div>\r\n            <div className=\"item\">\r\n                <ShareOutlinedIcon/>\r\n            </div>\r\n        </div>\r\n        {comments && <Comments postId={post.Id} />}\r\n        </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Post;","C:\\Users\\ADMIN\\Desktop\\ReactJS\\Socio App\\Client\\src\\components\\comments\\Comments.jsx",[],[],"C:\\Users\\ADMIN\\Desktop\\ReactJS\\Socio App\\Client\\src\\components\\share\\Share.jsx",[],[],"C:\\Users\\ADMIN\\Desktop\\ReactJS\\Socio App\\Client\\src\\axios.js",[],[],"C:\\Users\\ADMIN\\Desktop\\ReactJS\\Socio App\\Client\\src\\components\\update\\Update.jsx",["243"],[],"C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\index.js",[],[],"C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\App.js",["244"],[],"import Login from \"./pages/login/Login\";\nimport Register from \"./pages/register/Register\";\nimport {\n  createBrowserRouter,\n  RouterProvider,\n  Route,\n  Outlet,\n  Navigate,\n} from \"react-router-dom\";\nimport Navbar from \"./components/navbar/navbar\";\nimport RightBar from \"./components/rightBar/Rightbar\";\nimport Home from \"./pages/home/Home\";\nimport Profile from \"./pages/profile/Profile\";\nimport \"./style.scss\";\nimport { useContext } from \"react\";\nimport { DarkModeContext } from \"./context/darkModeContext\";\nimport { AuthContext } from \"./context/authContext\";\nimport Leftbar from \"./components/leftBar/Leftbar\";\nimport \"./app.scss\"\nimport {\n  QueryClient,\n  QueryClientProvider,\n} from '@tanstack/react-query'\n\nfunction App() {\n  const {currentUser} = useContext(AuthContext);\n\n  const { darkMode } = useContext(DarkModeContext);\n\n  const queryClient = new QueryClient()\n  const Layout = () => {\n    return (\n      <QueryClientProvider client={queryClient}>\n      <div className={`theme-${darkMode ? \"dark\" : \"light\"}`} >\n        <div className=\"layout\">\n        <Navbar />\n        <div style={{ display: \"flex\" }}>\n          <Leftbar />\n          <div style={{ flex: 6 }}>\n            <Outlet />\n          </div>\n          <RightBar />\n        </div>\n        </div>\n      </div>\n      </QueryClientProvider>\n    );\n  };\n\n  const ProtectedRoute = ({ children }) => {\n    if (!currentUser) {\n      return <Navigate to=\"/login\" />;\n    }\n\n    return children;\n  };\n\n  const router = createBrowserRouter([\n    {\n      path: \"/\",\n      element: (\n        <ProtectedRoute>\n          <Layout />\n        </ProtectedRoute>\n      ),\n      children: [\n        {\n          path: \"/\",\n          element: <Home />,\n        },\n        {\n          path: \"/profile/:id\",\n          element: <Profile />,\n        },\n      ],\n    },\n    {\n      path: \"/login\",\n      element: <Login />,\n    },\n    {\n      path: \"/register\",\n      element: <Register />,\n    },\n  ]);\n\n  return (\n    <div>\n      <RouterProvider router={router} />\n    </div>\n  );\n}\n\nexport default App;","C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\reportWebVitals.js",[],[],"C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\context\\darkModeContext.js",[],[],"C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\context\\authContext.js",[],[],"C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\pages\\login\\Login.jsx",["245"],[],"import React, { useContext,useState } from 'react'\nimport \"./login.scss\"\nimport { Link, useNavigate } from 'react-router-dom'\nimport { AuthContext } from '../../context/authContext'\nimport axios from 'axios'\n\nconst Login = () => {\n  const [inputs,setInputs] = useState({\n    username: \"\",\n    password: \"\",\n  })\n  const navigate = useNavigate()\n  const [err,setErr] = useState(null)\n  console.log(err)\n\n  const handleChange = (e) =>{\n    setInputs(prev => ({...prev, [e.target.name]: e.target.value}))\n  }\n  const {login}  = useContext(AuthContext)\n\n  const handleLoggin = async(e) =>{\n    e.preventDefault()\n    try{\n      await login(inputs)\n      navigate(\"/\")\n    }catch(err){\n      setErr(err.response.data)\n    } \n  }\n\n   \n\n  return (\n        <div className='login'>\n          <div className='card'>\n            <div className='left'>\n              <h1>Welcome to Socio </h1>\n              <p>We provide a platform where people can find and meet together</p>\n              <span>Don't you have an account? </span>\n              <Link to=\"/register\">\n              <button>Register</button>\n              </Link>\n            </div>\n\n            <div className='right'>\n              <h1>Login</h1>\n              <form>\n                <input type='text' placeholder='Username' name='username' onChange={handleChange}/>\n                <input type=\"password\" placeholder='Password'\n                name='password' onChange={handleChange}/>\n                {err && err}\n                <button onClick={handleLoggin}>Log in</button>\n              </form>\n            </div>\n          </div>\n        </div>\n  )\n}\n\nexport default Login\n","C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\pages\\home\\Home.jsx",[],[],"C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\pages\\register\\Register.jsx",[],[],"C:\\Users\\ADMIN\\Desktop\\Socio-media-platform\\Client\\src\\pages\\profile\\Profile.jsx",["246","247"],[],"import \"./profile.scss\";\nimport FacebookTwoToneIcon from \"@mui/icons-material/FacebookTwoTone\";\nimport LinkedInIcon from \"@mui/icons-material/LinkedIn\";\nimport InstagramIcon from \"@mui/icons-material/Instagram\";\nimport PinterestIcon from \"@mui/icons-material/Pinterest\";\nimport TwitterIcon from \"@mui/icons-material/Twitter\";\nimport PlaceIcon from \"@mui/icons-material/Place\";\nimport LanguageIcon from \"@mui/icons-material/Language\";\nimport EmailOutlinedIcon from \"@mui/icons-material/EmailOutlined\";\nimport MoreVertIcon from \"@mui/icons-material/MoreVert\";\nimport Posts from \"../../components/posts/Posts\"\nimport {\n  useQuery, useQueryClient, useMutation\n} from '@tanstack/react-query' \nimport { makeRequest } from \"../../axios\";\nimport { useLocation } from \"react-router-dom\";\nimport { useContext, useState } from \"react\";\nimport { AuthContext } from \"../../context/authContext\";\nimport Update from \"../../components/update/Update\";\n\nconst Profile = () => {\n  const [openUpdate,setOpenUpdate] = useState(false)\n  const queryClient = useQueryClient()\n  const {currentUser} = useContext(AuthContext)\n  const userId = parseInt(useLocation().pathname.split(\"/\")[2])\n  const { isLoading, error, data } = useQuery([\"user\"], () =>\n    makeRequest.get(\"/users/find/\" + userId).then((res) => {\n      return res.data;\n    })\n  );\n  const { isLoading: rIsLoading, data: relationshipData } = useQuery(\n    [\"relationship\"],\n    () =>\n      makeRequest.get(\"/relationships?followedUserId=\" + userId).then((res) => {\n        return res.data;\n      })\n  );\n  const mutation = useMutation(\n    (following) => {\n      if (following) return makeRequest.delete(\"/relationships?userId=\" + userId);\n      return makeRequest.post(\"/relationships\", {userId});\n    },\n    {\n      onSuccess: () => {  \n        // Invalidate and refetch\n        queryClient.invalidateQueries([\"relationship\"]);\n      },\n    }\n  );\n  let cover = data?.coverPic;\n  let profilePic = data?.profilePic;\n  let name = data?.name;\n  let city = data?.city;\n  let website = data?.website;\n  const handleFollow = () =>{\n    mutation.mutate(relationshipData.includes(currentUser.id));\n  }\n  return (\n    <div className=\"profile\">\n      <div className=\"images\">\n        <img\n          src={\"/upload/\"+cover}\n          alt=\"\"\n          className=\"cover\"\n        />\n        <img\n          src={\"/upload/\" + profilePic}\n          alt=\"\"\n          className=\"profilePic\"\n        />\n      </div>\n      <div className=\"profileContainer\">\n        <div className=\"uInfo\">\n          <div className=\"left\">\n            <a href=\"http://facebook.com\">\n              <FacebookTwoToneIcon fontSize=\"large\" />\n            </a>\n            <a href=\"http://facebook.com\">\n              <InstagramIcon fontSize=\"large\" />\n            </a>\n            <a href=\"http://facebook.com\">\n              <TwitterIcon fontSize=\"large\" />\n            </a>\n            <a href=\"http://facebook.com\">\n              <LinkedInIcon fontSize=\"large\" />\n            </a>\n            <a href=\"http://facebook.com\">\n              <PinterestIcon fontSize=\"large\" />\n            </a>\n          </div>\n          <div className=\"center\">\n            <span className=\"name\">{name}</span>\n            <div className=\"info\">\n              <div className=\"item\">\n                <PlaceIcon />\n                <span>{city}</span>\n              </div>\n              <div className=\"item\">\n                <LanguageIcon />\n                <span>{website}</span>\n              </div>\n            </div>\n            {rIsLoading \n            ? <div className=\"loading\">Loading</div> \n            : userId === currentUser.id \n            ? (<button onClick={()=>setOpenUpdate(true)}>Update</button>) \n            : (<button onClick={handleFollow}>{relationshipData.includes(currentUser.id) \n            ? \"Following\" \n            : \"Follow\"}\n            </button>)}\n          </div>\n          <div className=\"right\">\n            <EmailOutlinedIcon />\n            <MoreVertIcon />\n          </div>\n        </div>\n      <Posts userId={userId}/>\n      </div>\n      {openUpdate && <Update \n      setOpenUpdate={setOpenUpdate} \n      user={data}\n      />}\n    </div>\n  );\n};\n\nexport default Profile;","C:\\Users\\ADMIN\\Desktop\\Socio-media-plat